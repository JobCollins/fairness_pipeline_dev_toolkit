    "    params: {}\n",
    "  - name: di\n",
    "    transformer: \"DisparateImpactRemover\"\n",
    "    params:\n",
    "      features: [\"x1\"]\n",
    "      sensitive: \"group\"\n",
    "      repair_level: 0.8\n",
    "'''\n",
    "cfg = load_config(text=cfg_text)\n",
